<style>
.site-content-contain { position: relative; z-index: 1; }
#footer { position: relative; z-index: 2; }
.guides .container { width: 1340px; max-width: 90%; }

.guides .navbar { background-color: #3448c5; color: #FFF; border-radius: 0; border: none; margin: 0; position: fixed; width: 100%; z-index: 3; top: 70px; }
@media (max-width: 1070px) {
  .guides .navbar { top: 50px; }
}
.guides .navbar .dropdown { padding: 10px 15px 10px 0; position: static; }
@media (max-width: 991px) {
  .guides .navbar .dropdown { padding-right: 0; }
}
.guides .navbar .dropdown-toggle { color: #FFF; display: flex; align-items: center; justify-content: space-between; }
.guides .navbar .dropdown-menu { border-radius: 0; box-shadow: none; background-color: rgba(0,0,0,0); border: none; width: 100%; margin: 0; pointer-events: none; height: calc(100vh - (78px + 50px)); }
.guides .navbar .dropdown-menu .container, .guides .navbar .dropdown-menu .row, .guides .navbar .dropdown-menu .col-xs-12 { height: 100%; }
.guides .navbar .dropdown-menu:before { content: ""; display: block; position: absolute; left: 0; top: 0; bottom: 0; background-color: #4c64d7; width: 25%; }
.guides .navbar .dropdown-list { padding: 20px 0; margin: 0; list-style: none; background-color: #4c64d7; pointer-events: all; height: 100%; }
.guides .navbar .dropdown-list > li > a { color: #FFF; padding: 5px 0; display: block; }
#media-nav-icon {
  width: 14px;
  height: 12px;
  position: relative;
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  -webkit-transition: .5s ease-in-out;
  -moz-transition: .5s ease-in-out;
  -o-transition: .5s ease-in-out;
  transition: .5s ease-in-out;
  cursor: pointer;
  display: inline-block;
}
#media-nav-icon span {
  display: block;
  position: absolute;
  height: 1px;
  width: 100%;
  background: #FFF;
  border-radius: 1px;
  opacity: 1;
  left: 0;
  -webkit-transform: rotate(0deg);
  -moz-transform: rotate(0deg);
  -o-transform: rotate(0deg);
  transform: rotate(0deg);
  -webkit-transition: .25s ease-in-out;
  -moz-transition: .25s ease-in-out;
  -o-transition: .25s ease-in-out;
  transition: .25s ease-in-out;
}
#media-nav-icon span:nth-child(1) {
  top: 0px;
}
#media-nav-icon span:nth-child(2), #media-nav-icon span:nth-child(3) {
  top: 5px;
}
#media-nav-icon span:nth-child(4) {
  top: 10px;
}
.dropdown.open #media-nav-icon span:nth-child(1) {
  top: 18px;
  width: 0%;
  left: 50%;
}
.dropdown.open #media-nav-icon span:nth-child(2) {
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}
.dropdown.open #media-nav-icon span:nth-child(3) {
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  transform: rotate(-45deg);
}
.dropdown.open #media-nav-icon span:nth-child(4) {
  top: 18px;
  width: 0%;
  left: 50%;
}

.content-wrapper { padding: 100px 0 50px 0; }

.list-group { padding-right: 15px; }
@media (max-width: 767px) {
  .list-group { padding-right: 0; }
}
.list-group > .list-group-item { background-color: #f1f2f9; border: none; border-radius: 0; margin-bottom: 5px; color: #0C163B; font-family: sofia-pro, sans-serif; font-weight: 700; font-size: 15px; line-height: 1.5; border-left: solid 5px #f1f2f9; padding: 10px 25px; }
.list-group > .list-group-item.heading { font-size: 18px; padding: 5px 25px; }
.list-group > .list-group-item.active { border-color: #48c4d8; background-color: #c7cae3; }
.list-group > .list-group-item:not(.heading):hover { border-color: #48c4d8; background-color: #f1f2f9; color: #0C163B; }
.list-group > .list-group-item:not(.heading):focus { background-color: #f1f2f9; color: #0C163B; }

.content-wrapper .text h3 { margin-bottom: 30px; scroll-margin-top: 150px; }
.content-wrapper .text p + ul { margin-top: -25px; }
.content-wrapper .text ul { padding-left: 15px; margin-bottom: 30px; }
.content-wrapper .text ul li { margin-bottom: 5px; }
.content-wrapper .text a { color: #3448c5; text-decoration: underline; }
.content-wrapper .text .anchor-menu a { text-decoration: none; }

.references { background-color: #f1f2f9; border-radius: 24px; padding: 50px; margin: 50px -50px 0 -50px; }
.references h2 { margin-bottom: 30px; }
.references .items { list-style: none; padding: 0; margin: 0; }
.references .items a { color: #0C163B; text-decoration: underline; }

.cta { text-align: center; padding-left: 10%; }
.cta img { margin-bottom: 30px; }
.cta h4 { margin-bottom: 30px; }
.cta .btn.btn-default { background-color: #3448c5; border-color: #3448c5; padding: 8px 30px; }



#top { scroll-margin-top: 150px; }
.c-back-to-top {
  background-color: #45c5da;
  border-radius: 50%;
  color: #fff !important;
  display: flex;
  height: 38px;
  text-decoration: none;
  width: 38px;
}
.c-arrow-container__arrow {
  height: auto;
  width: 100%;
}
.c-arrow-container__arrow * {
  fill: none;
  stroke: currentColor;
  stroke-linecap: round;
  stroke-linejoin: round;
  stroke-width: 2.5px; // 2
}

@media (prefers-reduced-motion: no-preference) {
  html {
    scroll-behavior: smooth;
  }
}

.cta {
	position: -webkit-sticky;
	position: sticky;
	top: 160px;
}

.same_row {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display:         flex;
  flex-wrap: wrap;
}
.same_row > [class*='col-'] {
  display: flex;
  flex-direction: column;
}

/*QUICK TIPS*/
.top {
	border-radius: 7.496px 7.496px 0px 0px;
	background: linear-gradient(340deg, #0C163B 15.12%, #18246F 56.41%, #6F447E 94.45%);
	position: relative;
	min-height: 115px;
	padding-top:20px;
	padding-left: 30px;
}
.quick-tag {
	color: var(--Greys-White, #FFF);
