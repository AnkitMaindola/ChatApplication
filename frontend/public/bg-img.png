	margin-bottom: 0!important;
}
.pro-tip h3, .pro-tip a {
	color: white!important;
}
.pro-tip p {
	margin-bottom: 10px;
}     
.pro-tip-top {
	margin-bottom: 10px;
	display: flex;
}
.pro-tip img {
	width: 35px;
	height: auto;
	margin-right: 10px;
}     

@media (max-width: 767px) {
	.quick-tips-parent h5 {
		max-width: calc(100% - 110px);
	}
  	.quick-tips-parent img {
		width: 60px;
		position: absolute;
		right: 30px;
		top: 20px;
	}   
	.top:after {
		display: none;
	}
	.cta {
		margin-top: 40px;
	    padding-left: 0%;
	}
}

/*CASE STUDY*/
.case-study-parent {
	display: flex;
